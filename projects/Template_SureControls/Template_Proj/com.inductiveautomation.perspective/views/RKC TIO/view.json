          "name": "Label_PV"
        },
        "position": {
          "height": 30,
          "width": 60,
          "x": 80,
          "y": 140
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.PV"
              },
              "transforms": [
                {
                  "code": "\tstring \u003d str(value) + u\u0027 Â°F\u0027\n\treturn string",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Main/TagValue"
          },
          "textStyle": {
            "classes": "RKC/Text"
          }
        },
        "type": "ia.display.label"
      },
      {
        "custom": {
          "key": [
            {
              "aggregate": "Average",
              "alias": "PV"
            }
          ]
        },
        "meta": {
          "name": "Sparkline"
        },
        "position": {
          "height": 100,
          "width": 90,
          "x": 10,
          "y": 40
        },
        "propConfig": {
          "custom.key[0].path": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.Name"
              },
              "transforms": [
                {
                  "expression": "\u0027[edge]\u0027+{value}+\u0027/PV\u0027",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.desired.high": {
            "binding": {
              "config": {
                "expression": "if({view.params.udtElements.IndicateAndControl},{view.params.udtElements.ProcessHigh},600)"
              },
              "type": "expr"
            }
          },
          "props.desired.low": {
            "binding": {
              "config": {
                "expression": "if({view.params.udtElements.IndicateAndControl},{view.params.udtElements.ProcessLow},300)"
              },
              "type": "expr"
            }
          },
          "props.points": {
            "binding": {
              "config": {
                "aggregate": "MinMax",
                "avoidScanClassValidation": true,
                "dateRange": {
                  "mostRecent": "1",
                  "mostRecentUnits": "HOUR"
                },
                "enableValueCache": true,
                "ignoreBadQuality": false,
                "polling": {
                  "enabled": false,
                  "rate": "30"
                },
                "preventInterpolation": false,
                "returnFormat": "Wide",
                "returnSize": {
                  "numRows": "100",
                  "type": "FIXED"
                },
                "tags": "{this.custom.key}",
                "valueFormat": "DATASET"
              },
              "type": "tag-history"
            }
          },
          "props.range.high": {
            "binding": {
              "config": {
                "path": "this.props.desired.high"
              },
              "transforms": [
                {
                  "expression": "{value}+50",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.range.low": {
            "binding": {
              "config": {
                "path": "this.props.desired.low"
              },
              "transforms": [
                {
                  "expression": "{value}-50",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "desired": {},
          "range": {}
        },
        "type": "ia.display.sparkline"
      },
