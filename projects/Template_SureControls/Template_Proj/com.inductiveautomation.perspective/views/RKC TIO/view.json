        }
      ]
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "MultiState_Manual"
        },
        "position": {
          "height": 30,
          "width": 130,
          "x": 10,
          "y": 210
        },
        "propConfig": {
          "meta.visible": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.IndicateAndControl"
              },
              "type": "property"
            }
          },
          "props.controlValue": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.udtElements.Manual"
              },
              "type": "property"
            }
          },
          "props.indicatorValue": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.InManual"
              },
              "transforms": [
                {
                  "fallback": null,
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": true,
                      "output": 1
                    },
                    {
                      "input": false,
                      "output": 0
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "defaultSelectedStyle": {
            "classes": "RKC/Selected"
          },
          "defaultUnselectedStyle": {
            "classes": "RKC/Unselected"
          },
          "orientation": "row",
          "states": [
            {
              "selectedStyle": {
                "classes": ""
              },
              "text": "Auto",
              "unselectedStyle": {
                "classes": ""
              },
              "value": 0
            },
            {
              "selectedStyle": {
                "classes": ""
              },
              "text": "Manual",
              "unselectedStyle": {
                "classes": ""
              },
              "value": 1
            }
          ]
