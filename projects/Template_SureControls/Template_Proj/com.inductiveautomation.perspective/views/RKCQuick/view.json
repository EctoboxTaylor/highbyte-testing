{
  "custom": {},
  "params": {
    "udtElements": {
      "CV": "",
      "DeviationValue": "",
      "Name": "",
      "PV": "",
      "ProcessHigh": "",
      "ProcessLow": "",
      "SP": ""
    }
  },
  "propConfig": {
    "params.udtElements": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 70
    },
    "dropConfig": {
      "udts": [
        {
          "action": "path",
          "param": "udtElements",
          "type": "RKC TIO"
        }
      ]
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "MovingAnalogIndicator"
        },
        "position": {
          "height": 130,
          "width": 70
        },
        "propConfig": {
          "props.desiredHigh": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.DeviationValue"
              },
              "transforms": [
                {
                  "expression": "{view.params.udtElements.SP}+{value}",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.desiredLow": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.DeviationValue"
              },
              "transforms": [
                {
                  "expression": "{view.params.udtElements.SP}-{value}",
                  "type": "expression"
                }
              ],
              "type": "property"
            }
          },
          "props.highAlarm": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.ProcessHigh"
              },
              "type": "property"
            }
          },
          "props.lowAlarm": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.ProcessLow"
              },
              "type": "property"
            }
          },
          "props.maxValue": {
            "binding": {
              "config": {
                "expression": "{this.props.highAlarm}+50"
              },
              "type": "expr"
            }
          },
          "props.minValue": {
            "binding": {
              "config": {
                "expression": "{this.props.lowAlarm}-50"
              },
              "type": "expr"
            }
          },
          "props.processValue": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.PV"
              },
              "type": "property"
            }
          },
          "props.setpointValue": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.SP"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "highHighAlarm": null,
          "highInterlock": null,
          "lowInterlock": null,
          "lowLowAlarm": null
        },
        "type": "ia.display.moving-analog-indicator"
      },
      {
        "meta": {
          "name": "Label_Name"
        },
        "position": {
          "height": 30,
          "width": 60,
          "x": 10,
          "y": 160
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.Name"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Main/TagLabel"
          },
          "textStyle": {
            "classes": "RKC/Text"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_PV"
        },
        "position": {
          "height": 30,
          "width": 60,
          "x": 10,
          "y": 130
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.udtElements.PV"
              },
              "transforms": [
                {
                  "code": "\tstring \u003d str(value) + u\u0027 Â°F\u0027\n\treturn string",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Main/TagValue"
          },
          "textStyle": {
            "classes": "RKC/Text"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.coord"
  }
}